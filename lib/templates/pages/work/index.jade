extends ../../layouts/base

block seo-meta
  title Contact Me | Adam Gibbons
  meta(name="description" content="Want to learn more? Fill out this form, call, or email Adam.")

block seo-breadcrumbs
  script(type="application/ld+json").
    {
      "@context": "http://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "item": {
          "@id": "http://agibbons.com/",
          "name": "Adam Gibbons"
        }
      },
      {
        "@type": "ListItem",
        "position": 2,
        "item": {
          "@id": "http://agibbons.com/work/",
          "name": "Work"
        }
      }]
    }

block content
  .container
    ul
      li: a(href="/work/onemove") OneMove
      li: a(href="/work/moveline") Moveline
      li: a(href="/work/ics") ICS
